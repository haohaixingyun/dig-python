<?xml version="1.0" encoding="UTF-8"?>
<testcase name="NewRealease_VNovember_SalesOrder_01" id="1" actionDomain="web"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"> 
	<header> 
		<!-- 
		Header section includes informational stuff, which could be omitted.
		--> 
		<name>NewRealease_VNovember_SalesOrder_01</name>
		<author>Wizard</author>
		<version>1.0</version>
		<date>Mar 6th, 2008</date> 
		<time>15:00:00</time>
		<objective>......</objective> 
		<prerequisites> Some data, blahblah.</prerequisites>
		<info>One Sales Order Match.</info> 
	</header>
	<mapping-set>
		<mapping src="fastPassHomePage.xml" />
		<mapping src="loginPage.xml" />
		<mapping src="salesOrderSearchPage.xml" />
		<mapping src="salesOrderInfoPage.xml" />
		<mapping src="ServicesAgreementBillingOrders.xml" />
	</mapping-set>
	<data-set>
		<data src="SalesOrder_data.xml"></data>
	</data-set>
	<iter-policy>sequential</iter-policy>
	<casepolicy></casepolicy>
	<coverage></coverage>
	<casepasscriteria></casepasscriteria>
	<site></site>
	<dependentcases>
		<case src="NewRealease_VNovember_SalesOrder_01.xml"></case>
	</dependentcases>
	<logchannels>
		<logchannel item="logchannel1"></logchannel>
		<logchannel item="logchannel2"></logchannel>
	</logchannels>
	<reportchannels>
		<reportchannel item="report"></reportchannel>
	</reportchannels>
	<tags>
		<tag key="1"></tag>
		<tag key="2"></tag>
	</tags>
	
	<prolog>
  			Browser $web:fastPassHome.
  	</prolog> 
  	<steps>
  			<step index="1" comment="Click the 'Sales Orders' option from the navigation panel." extension="The login page should show">
				Click fastPassHomePage:salesOrders_link.
				<verify type="object_existence" object="loginPage:submit_button" value="60" retry="1"/>
			</step>
  			<step index="2" comment="Login FastPass." extension="Sales Order search form">
				settext loginPage:logon_text $web:username.
				settext loginPage:logon_password $web:password.
				Click loginPage:submit_button.
				<verify type="object_existence" object="salesOrderSearchPage:search_button" value="60" retry="1"/>
			</step>
			<step index="3" comment="Input 'SAP sales order number' field with '0053766300' and click 'Search'." extension="Sales Order information page">
				settext salesOrderSearchPage:services_agreement_number_text $web:salesOrderNum11.
				Click salesOrderSearchPage:search_button.
				<verify type="object_existence" object="salesOrderInfoPage:AssociatedBillingOrders_link" value="60" retry="1"/>
				<verify type="string_in_page" object="Agreement - Site number:" value="60" retry="1"/>
				<verify type="string_in_page" object="Termination date:" value="60" retry="1"/>
				<verify type="string_in_page" object="Acquisition migration item:" value="60" retry="1"/>
			</step>	
  			<step index="4" comment="Click the AssociatedBillingOrders_link." extension="Sales Order search form">
				Click salesOrderInfoPage:AssociatedBillingOrders_link.
				<verify type="string_in_page" object="Services agreement billing orders" value="60" retry="1"/>
			</step>
  			<step index="5" comment="Click the tag3381940_link." extension="Sales Order search form">
				Click ServicesAgreementBillingOrders:tag3381940_link.
				<verify type="string_in_page" object="Customer details" value="60" retry="1"/>
			</step>				
			<step index="6" comment="Hit back." extension="Sales order information - Search on opportunity number">
				Go back.
				<verify type="string_in_page" object="Services agreement billing orders" value="60" retry="1"/>
			</step>
  			<step index="7" comment="Click the tag0077080386_link." extension="Sales Order search form">
				Click ServicesAgreementBillingOrders:tag0077080386_link.
				<verify type="string_in_page" object="Services agreement" value="60" retry="1"/>
			</step>				
			<step index="8" comment="Hit back." extension="Sales order information - Search on opportunity number">
				Go back.
				<verify type="string_in_page" object="Services agreement billing orders" value="60" retry="1"/>
			</step>		
  			<step index="9" comment="Click the tag0053649954_link." extension="Sales Order search form">
				Click ServicesAgreementBillingOrders:tag0053649954_link.
				<verify type="string_in_page" object="Sales order information" value="60" retry="1"/>
			</step>				
			<step index="10" comment="Hit back." extension="Sales order information - Search on opportunity number">
				Go back.
				<verify type="string_in_page" object="Services agreement billing orders" value="60" retry="1"/>
			</step>		
  			<step index="11" comment="Click the tagD0M8PLL_link." extension="Sales Order search form">
				Click ServicesAgreementBillingOrders:tagD0M8PLL_link.
				<verify type="string_in_page" object="Part information" value="60" retry="1"/>
			</step>				
			<step index="12" comment="Hit back." extension="Sales order information - Search on opportunity number">
				Go back.
				<verify type="string_in_page" object="Services agreement billing orders" value="60" retry="1"/>
			</step>				
	</steps>
	<epilog>
	</epilog>
</testcase>